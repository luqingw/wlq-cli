{
  "name": "wlq-cli",
  "version": "1.0.0",
  "description": "base template",
  "author": "luqing",
  "private": false,
  "bin": {
    "test": "bin/test.js"
  },
  "scripts": {
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "lint": "eslint --fix --ext .js,.vue src",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "changelog": "conventional-changelog --config ./changelog -i CHANGELOG.md -s -r 0",
    "commit": "cz",
    "coverage": "jest --coverage",
    "dev": "vue-cli-service serve",
    "new": "plop",
    "preview": "node build/index.js --preview",
    "release": "./release.sh",
    "sonar": "node sonar-project.js",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test": "jest",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "axios": "0.18.1",
    "clipboard": "2.0.4",
    "core-js": "^3.6.5",
    "dayjs": "^1.6.9",
    "echarts": "4.2.1",
    "element-ui": "^2.15.1",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.15",
    "mockjs": "^1.1.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "query-string": "^6.13.1",
    "sass-loader": "^9.0.0",
    "vue": "^2.6.12",
    "vue-router": "3.0.2",
    "vue-template-compiler": "^2.6.12",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "animate.css": "^3.7.2",
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-auto-import-externals": "^0.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "chromedriver": "^76.0.0",
    "cm-show-invisibles": "^3.0.1",
    "commitizen": "^4.0.3",
    "commitlint-config-cz": "^0.13.2",
    "connect": "3.6.6",
    "copy-webpack-plugin": "^5.1.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "cz-customizable": "^6.3.0",
    "dayjs": "^1.8.19",
    "edgedriver": "^4.17134.1",
    "eslint": "5.15.3",
    "eslint-plugin-es-beautifier": "^1.0.1",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "^4.3.8",
    "jest": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "js-yaml": "^3.13.1",
    "lint-staged": "8.1.5",
    "lodash": "^4.17.15",
    "nightwatch": "^1.2.1",
    "nightwatch-api": "latest",
    "node-sass": "^4.14.1",
    "plop": "2.3.0",
    "postcss-aspect-ratio-mini": "^1.0.1",
    "postcss-assets": "^5.0.0",
    "postcss-import": "^12.0.1",
    "postcss-merge-rules": "^4.0.3",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-write-svg": "^3.0.1",
    "raven-js": "^3.27.2",
    "runjs": "^4.3.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "selenium-server": "^3.0.1",
    "selenium-server-standalone-jar": "^3.141.5",
    "serve-static": "^1.13.2",
    "sonarqube-scanner": "^2.8.0",
    "standard-version": "^9.1.1",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-cli-plugin-auto-commitlint": "^0.9.23",
    "vue-jest": "^3.0.4"
  },
  "bugs": {
    "url": "http://47.100.195.176:8099/zentao/product-all.html"
  },
  "changelog": {
    "authorName": true,
    "authorEmail": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "homepage": "https://github.com/luqingw/wlq-cli/#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jestSonar": {
    "soanr56x": false,
    "reportPath": "tests/unit/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "keywords": [
    "tw"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+git@192.168.130.184:galaxy-cli/web-cli.git"
  },
  "standard-version": {
    "scripts": {
      "precommit": "npm run changelog"
    },
    "skip": {
      "changelog": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
